{
  "description": "",
  "scripts": {
    "dev": "nodemon --watch src src/main.ts",
    "build": "tsc -p tsconfig.json",
    "migrate:make": "knex migrate:make -x ts --knexfile src/config/knexfile",
    "migrate:latest": "knex migrate:latest --knexfile src/config/knexfile",
    "seed:make": "knex seed:make --knexfile src/config/knexfile",
    "seed:run": "knex seed:run --knexfile src/config/knexfile"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "fastify-plugin": "^4.5.1",
    "knex": "^3.1.0",
    "mssql": "^10.0.1"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
